    "popper.js": "^1.16.1",
    "pretty-checkbox-vue": "^1.1.9",
    "vue": "^2.6.12",
    "vue-loading-overlay": "^3.4.2",
    "vue-slider-component": "^3.2.11"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
